{
    "name": "digiton-ma/installer",
    "description": "Digiton's own Laravel starter-kits installer.",
    "keywords": ["laravel", "digiton-ma"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Abdelhamid Errahmouni",
            "email": "abdelhamid.errahmouni@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^10.20|^11.0",
        "illuminate/support": "^10.20|^11.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/process": "^6.2|^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4"
    },
    "bin": [
        "bin/digitoncli"
    ],
    "autoload": {
        "psr-4": {
            "Digitonma\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Digitonma\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
